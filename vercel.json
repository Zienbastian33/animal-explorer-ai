{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "$OPENAI_API_KEY",
    "IMAGE_GENERATION_FUNCTION_URL": "$IMAGE_GENERATION_FUNCTION_URL"
  }
}
